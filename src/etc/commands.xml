<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:MX_ModuleHelperBar:etc/command.xsd">
    <command name="clear_cache">
        <resource>Magento_Backend::cache</resource>
        <label>Clear Cache</label>
        <handleUrl>helperbar/ajax_cache/massRefresh</handleUrl>
        <options>MX\HelperBar\Model\Commands\Options\ClearCache</options>
    </command>
    <command name="template_path_hints">
        <resource>Magento_Config::dev</resource>
        <label>Template Path Hints</label>
        <handleUrl>helperbar/ajax_config/templatePathHints</handleUrl>
        <options>MX\HelperBar\Model\Commands\Options\TemplatePathHints</options>
    </command>
</config>
